#echoModeButton {
    background: transparent;
    qproperty-flat: true;
    margin-right: 4px;
    border: none;
    border-width: 0;
    image: url(:/image/assets/hide-password.png);
    max-width: 16px;
    max-height: 16px;
    margin-right: 5px;
}
#echoModeButton::checked {
    image: url(:/image/assets/show-password.png);
}

