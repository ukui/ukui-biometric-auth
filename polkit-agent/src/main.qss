#btnCancel, #btnAuth {
    min-height: 20px;
    min-width: 28px;
    font-size:14px;
    font-family: PingFangSC-Regular, PingFang SC;

    font-weight: 400;
    line-height: 20px;
}

#btnDetails {
    icon-size: 14px;
}

#lwDevices {
}
#lwDevices::item {
    height: 30px;
}

#lblHeader{
    font-size: 18px;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;

    line-height: 25px;
}

